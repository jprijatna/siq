<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="com.deloitte.smartservice.SMARTSERVICE.controller.CallLog" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Call Log">
		<content>
			<Panel headerText="Speech Analytics" height="18rem" width="auto">
				<HBox width="100%" class="hbox_tile_LookAlike" height="100%">
					<HBox width="auto">
						<TextArea enabled="false" id="transcriptText" width="100%" height="85%" editable="false" class="LongTxtInfo jobCardLongTxt"
							value=""></TextArea>
					</HBox>
				</HBox>
			</Panel>
			<Button text="Ticket" press="sendTicket"/>
			<Button text="Real-Time" press="startReceive"/>
			<Button text="Stop" press="stopReceive" />
		</content>
	</Page>
</mvc:View>